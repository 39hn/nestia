{
  "name": "nestia",
  "version": "1.1.0",
  "description": "Automatic SDK and Document generator for the NestJS",
  "main": "src/index.ts",
  "bin": {
    "nestia": "./src/bin/nestia.ts"
  },
  "scripts": {
    "dev": "tsc --watch",
    "test": "cd test && bash script.sh",
    "test:default": "cd test/default && rimraf -rf src/api/functional && ts-node ../../src/bin/nestia sdk src/controllers --out src/api"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samchon/nestia"
  },
  "keywords": [
    "nestjs",
    "sdk",
    "api",
    "document",
    "swagger"
  ],
  "author": "Jeongho Nam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samchon/nestia/issues"
  },
  "homepage": "https://github.com/samchon/nestia#readme",
  "dependencies": {
    "@types/cli": "^0.11.19",
    "@types/glob": "^7.2.0",
    "@types/node": "^14.14.22",
    "@types/reflect-metadata": "^0.1.0",
    "cli": "^1.0.1",
    "del": "^6.0.0",
    "glob": "^7.2.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.14.1",
    "tstl": "^2.5.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "nestia-helper": "^1.0.0",
    "rimraf": "^3.0.2"
  }
}
